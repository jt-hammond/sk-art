<%= link_to artwork_path(artwork) do %>
  <div class="card" style="display: inline-block; height: 300px;">
    <div class="card-img" id="img" style="background-image: url('<%= cl_image_path artwork.path %>'); height: 300px; display: inline-block;">
    </div>
    <span class="card-content">
      <h2><%= artwork.title %></h2>
    </span>
  </div>
<% end %>
